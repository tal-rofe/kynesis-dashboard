{
	"name": "@kynesis/frontend",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"postinstall": "playwright install --with-deps",
		"build": "dotenv -e envs/.env.production -- next build",
		"start:dev": "dotenv -e envs/.env.development -- next dev -p 8080",
		"start:dev:docker": "next dev -p 8080",
		"start:prod": "next start -p 8080",
		"lint": "eslint -c ./.eslintrc.cjs --ignore-path ./.eslintignore \"./**/*.{ts,tsx,cjs}\"",
		"type-check": "tsc --project ./tsconfig.json",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build --quiet",
		"storybook:test": "test-storybook",
		"chromatic": "npx chromatic --exit-zero-on-changes",
		"template:page": "hygen component new-page",
		"template:module": "hygen component new-module",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui"
	},
	"dependencies": {
		"@radix-ui/react-accordion": "1.1.2",
