	"private": true,
	"description": "Pixel API",
	"author": "Kynesis <dev@kynesis.io> (https://kynesis.io)",
	"license": "UNLICENSED",
	"type": "module",
	"scripts": {
		"postinstall": "prisma generate --schema ../../prisma/schema.prisma",
		"build": "nest build",
		"start:dev": "dotenv -e ./envs/.env.development -e ../../prisma/.env.development -- nest start --watch",
		"compodoc": "compodoc && open-cli ./documentation/index.html",
		"lint": "eslint -c ./.eslintrc.cjs --ignore-path ./.eslintignore \"./**/*.ts\"",
		"type-check": "tsc --project ./tsconfig.json"
	},
	"dependencies": {
		"@nestjs/common": "10.3.8",
		"@nestjs/config": "3.2.2",
		"@nestjs/core": "10.3.8",
		"@nestjs/platform-express": "10.3.8",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.1",
		"helmet": "7.1.0",
		"reflect-metadata": "0.2.2",
		"rxjs": "7.8.1"
	},
	"devDependencies": {
		"@compodoc/compodoc": "1.1.24",
		"@nestjs/cli": "10.3.2",
		"@nestjs/swagger": "7.3.1",
		"@prisma/client": "5.14.0",
		"@total-typescript/ts-reset": "0.5.1",
		"@types/webpack-node-externals": "3.0.4",
		"@typescript-eslint/eslint-plugin": "7.8.0",
		"@typescript-eslint/parser": "7.8.0",
		"dotenv-cli": "7.4.2",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-deprecation": "2.0.0",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-unicorn": "52.0.0",
		"eslint-plugin-unused-imports": "3.2.0",
		"open-cli": "8.0.0",
		"ts-loader": "9.5.1",
		"typescript": "5.4.5",
		"webpack": "5.91.0",
		"webpack-node-externals": "3.0.0"
	}
